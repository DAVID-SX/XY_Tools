<Window x:Class="TabManagerProject.TabManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TabManagerProject"
        mc:Ignorable="d"
        Title="插件管理器" Height="400" Width="350" Name="TabManagerWindow1" Loaded="TabManagerWindow1_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="75"/>
        </Grid.RowDefinitions>
        <GroupBox Name="tabNameGroupBox" Header="请勾选需要显示的面板" Margin="5">
            <StackPanel x:Name="tabNameStackPanel" />
        </GroupBox>
        <Button x:Name="confirmButton" Grid.Row="1" Content="确认" Height="35" Click="confirmButton_Click" Margin="50,20,214,20"/>
        <Button x:Name="cancelButton" Grid.Row="1" Content="取消" Height="35" Margin="209,20,58,20" Click="cancelButton_Click"/>
    </Grid>
</Window>
